package parser;

usecasediagram BatteryLoading {
  @Driver --
    Pay,
    RequestLoading;

  @Car --
    CheckLoadingCable,
    CalculateLoadingAmount,
    Loading,
    FastLoading;

  @LoadingStation --
    Pay,
    CheckLoadingCable,
    CalculateLoadingAmount,
    RequestLoading,
    Loading,
    FastLoading;

  FastLoading [isFastLoadingStation] --> Loading [isFastLoadingStation];
  App extends Pay;
  CreditCard extends Pay;

  RequestLoading --> CalculateLoadingAmount;
  CalculateLoadingAmount --> CheckLoadingCable;
}